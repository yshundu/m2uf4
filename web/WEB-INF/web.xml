<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>act1</servlet-name>
        <servlet-class>m2.act1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>act1part2</servlet-name>
        <servlet-class>act1part2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>act1_2</servlet-name>
        <servlet-class>m2.act1_2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>act1_3</servlet-name>
        <servlet-class>m2.act1_3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>act1</servlet-name>
        <url-pattern>/act1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>act1part2</servlet-name>
        <url-pattern>/act1part2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>act1_2</servlet-name>
        <url-pattern>/act1_2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>act1_3</servlet-name>
        <url-pattern>/act1_3</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
